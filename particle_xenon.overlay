&uart0 {
    /delete-property/ rts-pin;
    /delete-property/ cts-pin;
};

zephyr_udc0: &usbd {
    compatible = "nordic,nrf-usbd";
    status = "okay";
    cdc_acm_0: cdc_acm_uart0 {
        compatible = "zephyr,cdc-acm-uart";
        label = "CDC_ACM_0";
    };
};

/ {
    chosen {
        zephyr,console = &cdc_acm_0;
        zephyr,shell-uart = &cdc_acm_0;
    };
};